{
  "extends": ["next/core-web-vitals", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "no-unused-vars": "error",
    "no-console": "error",
    "prefer-const": "error",
    "no-unreachable": "error",
    "no-duplicate-imports": "error",
    "prettier/prettier": "error"
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  }
}
